algoritmo "conta luz"


var
   valor_conta:real
   
inicio

    escreva("Escreva o valor da conta: ")
    leia(valor_conta)
    
    se(valor_conta > 80)entao
       escreva("Você está gastando muito!!!")
     senao
       escreval("Seu gasto foi normal!!!")
    fimse
    
fimalgoritmo
